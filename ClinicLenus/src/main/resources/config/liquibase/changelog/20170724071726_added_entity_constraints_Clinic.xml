<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Clinic.
    -->
    <changeSet id="20170724071726-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="clinic_details_id"
                                 baseTableName="clinic"
                                 constraintName="fk_clinic_clinic_details_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clinic_details"/>

        <addForeignKeyConstraint baseColumnNames="receptionist_id"
                                 baseTableName="clinic"
                                 constraintName="fk_clinic_receptionist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="receptionist"/>

        <addForeignKeyConstraint baseColumnNames="doctor_id"
                                 baseTableName="clinic"
                                 constraintName="fk_clinic_doctor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="doctor"/>

    </changeSet>
</databaseChangeLog>
